<?xml version="1.0" encoding="utf-8"?>
<c:schema xmlns="http://www.w3.org/2001/XMLSchema">

	<c:element name="Papers" type="PaperType" />

	<c:complexType name="PaperType">
		<c:sequence>
			<c:element name="title" type="string" />
			<c:element name="type" type="string" />
			<c:element name="monthly" type="boolean" />
			<c:element name="chars" type="CharsType" />
		</c:sequence>
	</c:complexType>

	<c:complexType name="CharType">
		<c:sequence>
			<c:element name="color" type="boolean" />
			<c:element name="numberOfPage" type="integer" />
			<c:element name="glossy" type="boolean" />
			<c:element name="subscriptionIndex" type="int" />
		</c:sequence>
	</c:complexType>
</c:schema>